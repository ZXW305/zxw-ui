{
  "name": "@zengxianwen/zxw-ui",
  "version": "0.1.2",
  "private":false,
  "main":"./dist/zxw-ui.common.js",
  "files":[
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
